	"dependencies": {
		"@fontsource-variable/outfit": "^5.0.8",
		"@iconify/svelte": "^3.1.4",
		"@lucia-auth/adapter-prisma": "^3.0.2",
		"@prisma/client": "5.5.2",
		"lucia": "^2.7.2",
		"svelte-exmarkdown": "^3.0.1"
	}
